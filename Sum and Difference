#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int a,b;
    float c,d;
    scanf("%d %d",&a,&b);
    scanf("%f %f",&c,&d);
    int s=a+b;
    int d1=a-b;
    float s1=c+d;
    float d2=c-d;
    printf("%d %d" , s, d1);
    printf("\n");
    printf("%.1f %.1f" , s1," ", d2);
    return 0;
}
    
