#include <stdio.h>
int max_of_four(int,int,int,int);
int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}
int max_of_four(int p,int q,int r, int s){
    int g1= p>q?p:q;
    int g2 = r>s?r:s;
    int g = g1>g2?g1:g2;
    return g;
}
